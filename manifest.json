{
  "manifest_version": 2,

  "name": "Open Tab",
  "description": "This extension opens a tab in a new window.",
  "version": "1.0",

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background":{
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}
